{{# partial "content"}}
    <dl class="dl-horizontal">
        <dt>PID</dt>
        <dd>{{pid}}</dd>
        <dt>Name</dt>
        <dd>{{name}}</dd>
        <dt>Provider</dt>
        <dd>{{provider}}</dd>
        <dt>Loaded agent classes</dt>
        <dd>
            <ul>
                {{#each loadedAgentClasses}}
                    <li>{{this}}</li>
                {{/each}}
            </ul>
        </dd>
    </dl>
{{/partial}}

{{> vm/layout}}
